PORT=5000
NODE_ENV=development
CHROME_PATH=C:\Program Files\Google\Chrome\Application\chrome.exe

# Reconexión automática
RECONNECT_DELAY=10000          # Delay base para reconexión (ms)
MAX_RETRIES=5                # Máximo de intentos de reconexión
BACKOFF_MULTIPLIER=2          # Multiplicador para backoff exponencial
MAX_RECONNECT_DELAY=300000    # Delay máximo de reconexión (5 min)

MAX_RETRIES=3
PASS_ENCRYPTED=Nigga&Lopez&Mora

# Monitoreo (Watchdog)
WATCHDOG_INTERVAL=60000       # Intervalo de health check (30 seg)
HEALTH_CHECK_TIMEOUT=10000     # Timeout para verificaciones (5 seg)
MAX_STATE_AGE=900000          # Edad máxima de estado (10 min)
MAX_QR_AGE=180000             # Tiempo máximo esperando QR (2 min)
MAX_INIT_TIME=300000          # Tiempo máximo de inicialización (3 min)
ZOMBIE_TIMEOUT=600000         # Timeout para detectar zombie (5 min)

# Browser Health Check
ENABLE_BROWSER_HEALTH_CHECK=false  # Deshabilitar health check agresivo
BROWSER_CHECK_INTERVAL=300000      # Si se habilita: 5 minutos (antes 1)
PAGE_RESPONSE_TIMEOUT=10000    # Timeout respuesta de página (3 seg)

# Rate Limiting
MESSAGE_RATE_LIMIT=1000       # Milisegundos entre mensajes

# Prometheus
ENABLE_PROMETHEUS=true
METRICS_PORT=9090

# Logging
LOG_LEVEL=info